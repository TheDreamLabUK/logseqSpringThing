{
  "name": "logseq-spring-thing",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --mode production",
    "build:quest": "vue-tsc && vite build --mode production --config vite.config.ts --platform quest",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "three": "0.170.0",
    "vue": "3.5.13",
    "pinia": "2.2.6",
    "vue-threejs": "0.2.0-alpha.1"
  },
  "devDependencies": {
    "@types/node": "22.9.3",
    "@types/three": "0.170.0",
    "@vitejs/plugin-vue": "5.2.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.0",
    "prettier": "^3.2.5",
    "typescript": "5.7.2",
    "vite": "5.4.11",
    "vue-tsc": "2.1.10"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "resolutions": {
    "three": "0.170.0",
    "@types/three": "0.170.0"
  }
}
